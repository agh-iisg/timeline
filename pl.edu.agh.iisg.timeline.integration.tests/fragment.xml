<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="pl.edu.agh.iisg.timeline.integration.TimelineViewTest"
            id="pl.edu.agh.iisg.timeline.integration.tests.view2"
            name="Timeline View Test"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="pl.edu.agh.iisg.timeline.integration.handler.CreateSampleTimelineDiagramHandler"
            id="pl.edu.agh.iisg.timeline.integration.tests.create"
            name="Create a sample diagram">
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
               name="Axes Count">
         </commandParameter>
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
               name="Elements Count">
         </commandParameter>
      </command>
      <command
            defaultHandler="pl.edu.agh.iisg.timeline.integration.handler.CreateRealDataTimelineDiagramHandler"
            id="pl.edu.agh.iisg.timeline.integration.tests.create.real.data"
            name="Create a real data diagram">
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
               name="Axes Count">
         </commandParameter>
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
               name="Elements Count">
         </commandParameter>
      </command>
      <command
            defaultHandler="pl.edu.agh.iisg.timeline.integration.handler.CreateLongTextsAndIconsTimelineDiagramHandler"
            id="pl.edu.agh.iisg.timeline.integration.tests.create.long.labels.icons"
            name="Create long texts and icons diagram">
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
               name="Axes Count">
         </commandParameter>
         <commandParameter
               id="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
               name="Elements Count">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="Timeline"
               mnemonic="T">
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create"
                  label="Create a small diagram"
                  mnemonic="s"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="3">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="100">
               </parameter>
            </command>
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create"
                  label="Create a medium diagram"
                  mnemonic="m"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="3">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="1000">
               </parameter>
            </command>
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create"
                  label="Create a large diagram"
                  mnemonic="l"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="5">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="10000">
               </parameter>
            </command>
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create"
                  label="Create a huge diagram"
                  mnemonic="h"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="10">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="100000">
               </parameter>
            </command>
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create.long.labels.icons"
                  label="Create diagram with icons and long texts"
                  mnemonic="L"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="3">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="100">
               </parameter>
            </command>
            <command
                  commandId="pl.edu.agh.iisg.timeline.integration.tests.create.real.data"
                  label="Create real data diagram"
                  mnemonic="r"
                  style="push">
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.axesCnt"
                     value="3">
               </parameter>
               <parameter
                     name="pl.edu.agh.iisg.timeline.create.sample.elementsCnt"
                     value="100">
               </parameter>
            </command>
         </menu>
      </menuContribution>
   </extension>

</fragment>
